# hdoc
hdoc "$"
hdoc con map        # searches text con and map, like concatMap but not map
hdoc ::a            # find type a
hdoc a -> a         # search by type

# hgdoc or 0xMF-ghci-doc
hgdoc "GHC.Base"
hgdoc "Control.Monad"
hgdoc "Data.List"

